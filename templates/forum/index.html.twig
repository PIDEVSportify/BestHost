{% extends 'base.html.twig' %}

{% block title %}Hello ForumController!{% endblock %}

{% block body %}



<body ng-controller="HomeController as home" ng-cloak class="home-page">
    <div id="home">
        <header class="header   page-home">




            <div class="header-left">
                <a href="index.html" class="header-logo">BestHost</a>
            </div>


        </header>

        <div id="home-wrapper" ng-init="people_to_follow(true); answer_favorites(null, null); answer_likes(null, null); current_user_id = null;" class="home-main">
            <div id="collection" class="question-profile">
                <div id="collection-topics">
                    <div class="collection-topic inner new-questions-heading hero-title">
                        <div class="intro">
                            <h2 class="title">Playbook helps designers achieve their career goals.</h2>
                        </div>
                    </div>

                    <div id="home-cards">
                        <ul class="collection-cards">
                        {% for categories  in category %}
                            <li class="collection-card">
                                <a class="collection-link" href="collection/get-your-first-product-design-job.html"></a>
                                <div id="collection-hero">
                                    <div class="collection-hero-inner" style="background-color: #f2e8e1; background-image: url(../askplaybook.s3.amazonaws.com/collections/covers/2_coverf113.png?1546817674);">
                                        <div class="collection-hero-left">
                                            <h1 class="collection-title">{{categories.name}}</h1>

                                            <div class="bundle">
                                                <ul class="contributor-group">
                                                    <li class="head" style="background-image: url(../askplaybook.s3.amazonaws.com/avatars/24_avatar8d7f.jpg?1516069077), url(https://s3-us-west-1.amazonaws.com/askplaybook/avatars/account-placeholder-square.png); border-color: #f2e8e1;" data-type="profile-picture"></li>
                                                    <li class="head" style="background-image: url(https://askplaybook.s3.amazonaws.com/avatars/2196_avatar.?1506556594), url(https://s3-us-west-1.amazonaws.com/askplaybook/avatars/account-placeholder-square.png); border-color: #f2e8e1;" data-type="profile-picture"></li>
                                                    <li class="head" style="background-image: url(../askplaybook.s3.amazonaws.com/avatars/2105_avatar84df.png?1510760681), url(https://s3-us-west-1.amazonaws.com/askplaybook/avatars/account-placeholder-square.png); border-color: #f2e8e1;" data-type="profile-picture"></li>
                                                </ul>
                                                <span class="stats">21 contributors</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </li>
                            {% endfor %}
                        </ul>
</div>
<form action="{{path('forum_create')}}">
<button type="submit" style="margin-left:50px;" class="btn btn-primary">add Post</button>
</form>
                        <div class="action-items responses">
                        <ul>
                        {% for Posts  in Post %}

<li ng-class="{ 'favorited': answer_favs.includes(233), 'liked': answer_likes.includes(233) }" ng-init="answer_233_action_items_limit = 3" id="answer-233">
  <!--<a href="/decide-if-freelancing-is-for-you/answers" class="action-item-click"></a>-->
  <div class="person">
    <a href="colbay.html">
    <span class="name">{{Posts.user}}</span>
    </a>
  </div>

  <ul class="answer-action-items ">
    
      <li class="action-0" ng-hide="0 > answer_233_action_items_limit - 1" ng-init="answer_233_action_0_see_more = false">
        <h4>{{Posts.title}}</h4>

        <p><span class="first">{{Posts.content}}</p>
      </li>
                    <a href="{{ path('forum_edit', {'id': Posts.id}) }}" class="btn btn-light">edit</a>
  <a href="{{ path('forum_delete', {'id': Posts.id}) }}" class="btn btn-light">delete</a>
        {% endfor %}
    


  </ul>




